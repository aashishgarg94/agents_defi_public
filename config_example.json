{
    "1":{
        "strategy_name":"ETH_USDC_Automated_Rebalance_Strategy",
        "execution_frequency":"every_10_minutes",
        "liquidity_split":{
            "ETH":50,
            "USDC":50
        },
        "market_conditions":{
            "current_price":"$1,800",
            "liquidity_depth":"$1,000,000",
            "slippage":"0.5%",
            "fee_earnings":"$5,000"
        },
        "technical_indicators":{
            "RSI":{
                "period":14,
                "thresholds":{
                    "overbought":70,
                    "oversold":30
                }
            },
            "MACD":{
                "fast_length":12,
                "slow_length":26,
                "signal_length":9
            },
            "Bollinger_Bands":{
                "period":20,
                "deviation":2
            },
            "Moving_Average":{
                "length":50
            },
            "VWAP":{
                "lookback_period":14
            },
            "Order_Flow":{
                "lookback_period":14
            }
        },
        "conditions":{
            "entry_conditions":{
                "must_meet":[
                    {
                        "indicator":"RSI",
                        "condition":"RSI < 30",
                        "action":"add_liquidity"
                    },
                    {
                        "indicator":"MACD",
                        "condition":"MACD line crosses above signal line",
                        "action":"add_liquidity"
                    },
                    {
                        "indicator":"Bollinger_Bands",
                        "condition":"Price touches lower Bollinger Band",
                        "action":"add_liquidity"
                    }
                ],
                "optional":[
                    {
                        "indicator":"VWAP",
                        "condition":"Price > VWAP",
                        "action":"add_liquidity"
                    },
                    {
                        "indicator":"Order_Flow",
                        "condition":"Net buy orders > Net sell orders",
                        "action":"add_liquidity"
                    }
                ]
            },
            "exit_conditions":{
                "must_meet":[
                    {
                        "indicator":"RSI",
                        "condition":"RSI > 70",
                        "action":"remove_liquidity"
                    }
                ],
                "optional":[
                    {
                        "indicator":"MACD",
                        "condition":"MACD line crosses below signal line",
                        "action":"remove_liquidity"
                    },
                    {
                        "indicator":"Bollinger_Bands",
                        "condition":"Price touches upper Bollinger Band",
                        "action":"remove_liquidity"
                    }
                ]
            },
            "rebalance_conditions":{
                "thresholds":{
                    "fee_earnings":"$1,000",
                    "liquidity_depth":"$500,000"
                },
                "action":"rebalance",
                "conditions":[
                    {
                        "indicator":"Moving_Average",
                        "condition":"Price crosses below Moving Average",
                        "action":"adjust_liquidity_range"
                    }
                ]
            },
            "stop_loss_conditions":{
                "must_meet":[
                    {
                        "indicator":"market_price",
                        "condition":"Price drops 10% below last rebalancing level",
                        "action":"close_all_positions"
                    }
                ]
            }
        },
        "risk_management":{
            "max_slippage":0.02,
            "stop_loss":{
                "enabled":true,
                "threshold":"10%",
                "execution":{
                    "action":"close_position",
                    "parameters":{
                        "exit_all_positions":true,
                        "market_order":true,
                        "slippage_tolerance":0.01
                    }
                }
            },
            "impermanent_loss_protection":{
                "enabled":true,
                "strategy":"hedging with put options",
                "parameters":{
                    "option_type":"put",
                    "strike_price":"5% below current ETH price",
                    "expiry":"30 days",
                    "premium_budget":"2% of liquidity allocation",
                    "execution":{
                        "auto_hedge":true,
                        "monitor_price":true,
                        "close_position_on_profit":"if option gains 50% value"
                    }
                }
            }
        }
    },
